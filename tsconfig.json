{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./build", // 컴파일 이후 JS파일이 생성될 디렉토리
    "rootDir": ".",
    "moduleResolution": "node"
  }
}
